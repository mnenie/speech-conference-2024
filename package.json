{
  "name": "keep-alive-vue-slidev",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "slidev build",
    "dev": "slidev --open",
    "export": "slidev export",
    "format": "prettier --write \"**/*.{vue,ts,js}\""
  },
  "slidev": {
    "colorSchema": "both",
    "highlighter": "shiki",
    "defaults": {
      "fonts": {
        "sans": "Source Sans Pro",
        "serif": "IBM Plex Serif",
        "mono": "IBM Plex Mono",
        "weights": [
          "200",
          "400",
          "600"
        ]
      },
      "hightlighter": "shiki"
    }
  },
  "dependencies": {
    "@iconify/json": "^2.2.205",
    "@slidev/cli": "^0.49.0-beta.4",
    "@slidev/theme-default": "latest",
    "@slidev/theme-seriph": "latest",
    "@slidev/types": "^0.49.0-beta.4",
    "raw-loader": "^4.0.2",
    "slidev-theme-vuetiful": "^0.3.1",
    "vue": "^3.4.22"
  },
  "devDependencies": {
    "@mnenie/prettier": "^1.0.4",
    "prettier-plugin-slidev": "^1.0.5"
  }
}
